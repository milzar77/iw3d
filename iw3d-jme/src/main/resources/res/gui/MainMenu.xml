<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="http://nifty-gui.lessvoid.com/nifty-gui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://raw.githubusercontent.com/void256/nifty-gui/1.4/nifty-core/src/main/resources/nifty.xsd https://raw.githubusercontent.com/void256/nifty-gui/1.4/nifty-core/src/main/resources/nifty.xsd">
    <useStyles filename="nifty-default-styles.xml"/>
    <useControls filename="nifty-default-controls.xml"/>
    <screen id="start" controller="com.blogspot.fravalle.iw3d.jme.simpleapplication.StartScreenController">
        <layer id="layer" backgroundColor="#0000" childLayout="vertical">
            <panel id="panel1" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="quit()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#f600" endColor="#ffff" post="true"/>
                </effect>
                <text id="text" font="aurulent-sans-16.fnt" color="#000f" text="Quit" align="center" valign="center" />
            </panel>
            <panel id="panel2" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="sampleCircularMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text2" font="aurulent-sans-16.fnt" color="#000f" text="Bookmark Circular Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel3" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="sampleQuadMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text3" font="aurulent-sans-16.fnt" color="#000f" text="Bookmark Quad Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel4" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="sampleRandomWebMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text4" font="aurulent-sans-16.fnt" color="#000f" text="Bookmark Random Web Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel6" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="surfCircularMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text5" font="aurulent-sans-16.fnt" color="#000f" text="Surf Circular Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel7" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="surfQuadMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text6" font="aurulent-sans-16.fnt" color="#000f" text="Surf Quad Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel8" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="center" visibleToMouse="true">
                <interact onClick="surfRandomWebMatrix()"/>
                <effect>
                    <onStartScreen name="move" mode="in" direction="top" length="300" startDelay="0" inherit="true"/>
                    <onEndScreen name="move" mode="out" direction="bottom" length="300" startDelay="0" inherit="true"/>
                    <onHover name="pulsate" scaleFactor="0.008" startColor="#0390" endColor="#ffff" post="true"/>
                </effect>
                <text id="text7" font="aurulent-sans-16.fnt" color="#000f" text="Surf Random Web Matrix" align="center" valign="center" />
            </panel>

            <panel id="panel5" height="5%" width="25%" align="right" valign="center" backgroundColor="#ffff" childLayout="vertical" visibleToMouse="true">
                <control id="domain" name="textfield" color="#000" text="www.google.com" width="100%" height="100%"/>
                <control id="surfDomain" name="button" color="#000" label="SURF" width="100%" height="100%">
                    <interact onClick="surf(domain)"/>
                </control>
            </panel>

        </layer>
    </screen>
    <screen id="end">
    </screen>
</nifty>